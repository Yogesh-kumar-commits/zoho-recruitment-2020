import java.util.*; 
class zoho
{    
public static void main(String[] args) 
{ 
    int input = 5, i, j, min; 
    System.out.println("Input: " + input); 
    for (i = 1; i <= input; i++)  
    { 
        for (j = 1; j <= input; j++)  
        { 
            min = i < j ? i : j; 
            System.out.print(input - min + 1); 
        } 
        System.out.println(); 
    } 
} 
} 
